@page
@model Frontend.Pages.CMunicipio.DeleteModel
@{
    ViewData["Title"]= "Eliminar Municipio";
}

<h2>Eliminar Municipio</h2>
@if(ViewData["Error"]==null)
{
    <h3 class="text-danger"> Esta seguro de eliminar este registro?</h3>
}
else
{
    <h3 class="text-danger">@ViewData["Error"]</h3>
}


<div class="form-horizontal">
    <hr/>
    <div class="row">
        <div class="col-md-4">
            <form method="post">
                <div class="form-group">
                    <label asp-for="Municipio.Id" class="control-label"></label>
                    <input asp-for="Municipio.Id" class="form-control" readonly />
                </div>

                <div class="form-group">
                    <label asp-for="Municipio.Nombre" class="control-label"></label>
                    <input asp-for="Municipio.Nombre" class="form-control" readonly  />
                </div>

                <div class="form-group">
                    <input type="submit" value="Eliminar" class="btn btn-danger"/>
                    <a asp-page="Index" class="btn btn-info">Cancelar</a>
                </div>

            </form>
        </div>
    </div>
</div>
